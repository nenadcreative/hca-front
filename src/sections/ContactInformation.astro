---
import Container from "../layouts/Container.astro";
import ButtonLink from "../components/ButtonLink.astro";
import { Image } from "astro:assets";

const { content } = Astro.props;

const bg = content.background;
const backgrounds = {
  gray: "bg-gray",
  white: "bg-white",
};
---

